{
  "name": "aurity-timesheet-app",
  "version": "1.0.0",
  "repository": "git@github.com:helios1138/aurity-timesheet-app.git",
  "author": "helios1138 <revan.den@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build-css": "node-sass src/ -o src/",
    "watch-css": "node-sass src/ -o src/ --source-map-embed=true --source-map-contents=true && node-sass src/ -o src/ --source-map-embed=true --source-map-contents=true --watch --recursive",
    "start": "react-scripts start",
    "build": "npm run build-css && react-scripts build",
    "serve": "serve -s build"
  },
  "devDependencies": {
    "node-sass": "^4.5.3",
    "react-scripts": "^1.0.7",
    "serve": "^5.1.5"
  },
  "dependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.3",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.5.3",
    "reselect": "^3.0.1",
    "whatwg-fetch": "^2.0.3"
  }
}
